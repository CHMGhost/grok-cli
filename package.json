{
  "name": "@chmgh0st/grok-cli",
  "version": "1.9.1",
  "description": "A code assistant CLI for Grok with markdown-based knowledge storage",
  "main": "dist/index.js",
  "bin": {
    "grok": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "cli",
    "grok",
    "ai",
    "code-assistant",
    "xai",
    "chatbot",
    "developer-tools",
    "typescript"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/grok-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/grok-cli/issues"
  },
  "homepage": "https://github.com/yourusername/grok-cli#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "ignore": "^5.3.0",
    "inquirer": "^9.2.12",
    "marked": "^11.1.0",
    "marked-terminal": "^6.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
